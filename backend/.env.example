# Supabase Configuration
SUPABASE_URL=your-project-url.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# JWT Configuration
JWT_SECRET=dev-jwt-secret-change-in-production
JWT_REFRESH_SECRET=dev-refresh-secret-change-in-production
JWT_EXPIRES_IN=7d

# Application Configuration
NODE_ENV=development
PORT=3001
FRONTEND_URL=http://localhost:5173

# AI Model Configuration
QWEN_API_KEY=placeholder
QWEN_API_URL=https://dashscope.aliyuncs.com/api/v1/services/aigc/text-generation/generation

BAICHUAN_API_KEY=placeholder
BAICHUAN_API_URL=https://api.baichuan-ai.com/v1/chat/completions

CHATGLM_API_KEY=placeholder
CHATGLM_API_URL=https://open.bigmodel.cn/api/paas/v4/chat/completions

OPENAI_API_KEY=placeholder
OPENAI_ENABLED=false

# File Upload Configuration
UPLOAD_DIR=./uploads
MAX_FILE_SIZE=10MB

# Feature Flags
ENABLE_AI_FEATURES=true
ENABLE_WORKFLOW_AUTOMATION=true
ENABLE_REAL_TIME_FEATURES=true
